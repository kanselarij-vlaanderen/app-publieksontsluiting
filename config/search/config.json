{
  "batch_size": 128,
  "max_batches": 0,
  "update_wait_interval_minutes": 0,
  "automatic_index_updates": true,
  "eager_indexing_groups": [
    [{"name": "public", "variables" : []}]
  ],
  "attachments_path_base": "/data/",
  "eager_indexing_sparql_query": false,
  "additive_indexes": true,
  "persist_indexes": true,
  "default_settings": {
    "analysis": {
      "analyzer": {
        "dutchanalyzer": {
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "dutchstemmer"
          ]
        }
      },
      "filter": {
        "dutchstemmer": {
          "type": "stemmer",
          "name": "dutch"
        }
      }
    }
  },
  "types": [
    {
      "type": "news-item",
      "on_path": "news-items",
      "rdf_type": "http://mu.semte.ch/vocabularies/ext/Nieuwsbericht",
      "properties": {
        "title": "http://purl.org/dc/terms/title",
        "alternativeTitle": "http://purl.org/dc/terms/alternative",
        "htmlContent": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#htmlContent",
        "position": "http://schema.org/position",
        "mandateeNames": [
          "http://www.w3.org/ns/prov#qualifiedAssociation",
          "http://data.vlaanderen.be/ns/mandaat#isBestuurlijkeAliasVan",
          "http://xmlns.com/foaf/0.1/name"
        ],
        "mandateeFirstNames": [
          "http://www.w3.org/ns/prov#qualifiedAssociation",
          "http://data.vlaanderen.be/ns/mandaat#isBestuurlijkeAliasVan",
          "https://data.vlaanderen.be/ns/persoon#gebruikteVoornaam"
        ],
        "mandateeFamilyNames": [
          "http://www.w3.org/ns/prov#qualifiedAssociation",
          "http://data.vlaanderen.be/ns/mandaat#isBestuurlijkeAliasVan",
          "http://xmlns.com/foaf/0.1/familyName"
        ],
        "mandateeIds": [
          "http://www.w3.org/ns/prov#qualifiedAssociation",
          "http://mu.semte.ch/vocabularies/core/uuid"
        ],
        "mandateeTitles": [
          "http://www.w3.org/ns/prov#qualifiedAssociation",
          "http://purl.org/dc/terms/title"
        ],
        "meetingDate": [
          "http://www.w3.org/ns/prov#wasDerivedFrom",
          "^http://purl.org/dc/terms/hasPart",
          "https://data.vlaanderen.be/ns/besluitvorming#isAgendaVoor",
          "http://data.vlaanderen.be/ns/besluit#geplandeStart"
        ],
        "meetingId": [
          "http://www.w3.org/ns/prov#wasDerivedFrom",
          "^http://purl.org/dc/terms/hasPart",
          "https://data.vlaanderen.be/ns/besluitvorming#isAgendaVoor",
          "http://mu.semte.ch/vocabularies/core/uuid"
        ],
        "themeId": [
          "http://purl.org/dc/terms/subject",
          "http://mu.semte.ch/vocabularies/core/uuid"
        ],
        "meetingPosition": [
          "http://www.w3.org/ns/prov#wasDerivedFrom",
          "http://schema.org/position"
        ],
        "agendaitemType": [
          "http://www.w3.org/ns/prov#wasDerivedFrom",
          "http://data.vlaanderen.be/ns/besluit#Agendapunt.type",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ]
      },
      "mappings": {
        "properties": {
          "title": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "alternativeTitle": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "htmlContent": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "position": {
            "type": "integer"
          },
          "mandateeNames": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "mandateeFirstNames": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "mandateeFamilyNames": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "mandateeIds": {
            "type": "keyword"
          },
          "mandateeTitles": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          },
          "meetingDate": {
            "type": "date"
          },
          "meetingId": {
            "type": "keyword"
          },
          "themeId": {
            "type": "keyword"
          },
          "meetingPosition": {
            "type": "integer"
          },
          "agendaitemType": {
            "type": "text",
            "analyzer": "dutchanalyzer",
            "search_analyzer": "dutchanalyzer"
          }
        }
      }
    }
  ]
}

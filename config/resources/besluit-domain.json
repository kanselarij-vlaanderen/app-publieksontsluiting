{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "besluitvorming": "https://data.vlaanderen.be/ns/besluitvorming#",
    "dbpedia": "http://dbpedia.org/ontology/",
    "dct": "http://purl.org/dc/terms/",
    "dossier": "https://data.vlaanderen.be/ns/dossier#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "mandaat": "http://data.vlaanderen.be/ns/mandaat#",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "person": "http://www.w3.org/ns/person#",
    "persoon": "https://data.vlaanderen.be/ns/persoon#",
    "prov": "http://www.w3.org/ns/prov#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "schema": "http://schema.org/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "themis": "http://themis.vlaanderen.be/vocabularies/besluitvorming/",
    "ext": "http://mu.semte.ch/vocabularies/ext/"
  },
  "resources": {
    "calendar-items": {
      "name": "calendar-item",
      "class": "besluit:Agendapunt",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "short-title": {
          "type": "string",
          "predicate": "besluitvorming:korteTitel"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "type": {
          "predicate": "besluit:Agendapunt.type",
          "target": "concept",
          "cardinality": "one"
        },
        "calendar": {
          "predicate": "dct:hasPart",
          "target": "calendar",
          "cardinality": "one",
          "inverse": true
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/agendapunt/"
    },
    "calendars": {
      "name": "calendar",
      "class": "besluitvorming:Agenda",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        }
      },
      "relationships": {
        "meeting": {
          "predicate": "besluitvorming:isAgendaVoor",
          "target": "meeting",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://themis-vlaanderen.be/id/agenda/"
    },
    "meetings": {
      "name": "meeting",
      "class": "besluit:Vergaderactiviteit",
      "attributes": {
        "planned-start": {
          "type": "datetime",
          "predicate": "besluit:geplandeStart"
        },
        "location": {
          "type": "string",
          "predicate": "prov:atLocation"
        },
        "identifier": {
          "type": "string",
          "predicate": "dct:identifier"
        },
        "planned-publication-date": {
          "type": "datetime",
          "predicate": "themis:geplandePublicatieDatumDocumenten"
        }
      },
      "relationships": {
        "type": {
          "predicate": "dct:type",
          "target": "concept",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://themis-vlaanderen.be/id/meeting/"
    },
    "concepts": {
      "name": "concept",
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "definition": {
          "type": "string",
          "predicate": "skos:definition"
        }
      },
      "relationships": {
        "in-scheme": {
          "target": "concept-scheme",
          "predicate": "skos:inScheme",
          "cardinality": "many"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://themis-vlaanderen.be/id/concept/"
    },
    "concept-schemes": {
      "name": "concept-scheme",
      "class": "skos:ConceptScheme",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "concepts": {
          "target": "concept",
          "predicate": "skos:inScheme",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://themis-vlaanderen.be/id/concept-scheme/"
    }
  }
}
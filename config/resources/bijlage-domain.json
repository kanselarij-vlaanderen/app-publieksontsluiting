{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "besluitvorming": "https://data.vlaanderen.be/ns/besluitvorming#",
    "dbpedia": "http://dbpedia.org/ontology/",
    "dct": "http://purl.org/dc/terms/",
    "dossier": "https://data.vlaanderen.be/ns/dossier#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "mandaat": "http://data.vlaanderen.be/ns/mandaat#",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "person": "http://www.w3.org/ns/person#",
    "persoon": "https://data.vlaanderen.be/ns/persoon#",
    "prov": "http://www.w3.org/ns/prov#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "schema": "http://schema.org/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "ext": "http://mu.semte.ch/vocabularies/ext/"
  },
  "resources": {
    "attachments": {
      "name": "attachment",
      "class": "dossier:Stuk",
      "attributes": {
        "title": {
          "type": "string",
          "predicate": "dct:title"
        },
        "created": {
          "type": "datetime",
          "predicate": "dct:issued"
        }
      },
      "relationships": {
        "file": {
          "predicate": "prov:value",
          "target": "file",
          "cardinality": "one"
        },
        "news-items": {
          "predicate": "besluitvorming:heeftBijlage",
          "target": "nieuws-item-info",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/resource/"
    },
    "files": {
      "name": "file",
      "class": "nfo:FileDataObject",
      "attributes": {
        "filename": {
          "type": "string",
          "predicate": "nfo:fileName"
        },
        "size": {
          "type": "number",
          "predicate": "nfo:fileSize"
        },
        "extension": {
          "type": "string",
          "predicate": "dbpedia:fileExtension"
        }
      },
      "relationships": {
        "download": {
          "predicate": "nie:dataSource",
          "target": "file",
          "cardinality": "one",
          "inverse": true
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/resource/"
    }
  }
}
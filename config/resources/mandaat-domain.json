{
  "version": "0.1",
  "prefixes": {
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "besluitvorming": "https://data.vlaanderen.be/ns/besluitvorming#",
    "dbpedia": "http://dbpedia.org/ontology/",
    "dct": "http://purl.org/dc/terms/",
    "dossier": "https://data.vlaanderen.be/ns/dossier#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "mandaat": "http://data.vlaanderen.be/ns/mandaat#",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#",
    "person": "http://www.w3.org/ns/person#",
    "persoon": "https://data.vlaanderen.be/ns/persoon#",
    "prov": "http://www.w3.org/ns/prov#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "schema": "http://schema.org/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "ext": "http://mu.semte.ch/vocabularies/ext/"
  },
  "resources": {
    "mandatees": {
      "name": "mandatee",
      "class": "mandaat:Mandataris",
      "attributes": {
        "position": {
          "type": "number",
          "predicate": "mandaat:rangorde"
        },
        "start-date": {
          "type": "datetime",
          "predicate": "mandaat:start"
        },
        "end-date": {
          "type": "datetime",
          "predicate": "mandaat:einde"
        },
        "title": {
          "type": "string",
          "predicate": "dct:title"
        }
      },
      "relationships": {
        "person": {
          "predicate": "mandaat:isBestuurlijkeAliasVan",
          "target": "person",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/mandataris/"
    },
    "people": {
      "name": "person",
      "class": "person:Person",
      "attributes": {
        "last-name": {
          "type": "string",
          "predicate": "foaf:familyName"
        },
        "first-name": {
          "type": "string",
          "predicate": "persoon:gebruikteVoornaam"
        },
        "alternative-name": {
          "type": "string",
          "predicate": "foaf:name"
        }
      },
      "relationships": {
        "mandatee": {
          "predicate": "mandaat:isBestuurlijkeAliasVan",
          "target": "mandatee",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://themis.vlaanderen.be/id/mandataris/"
    }
  }
}